{
  "name": "@shingo/shingo-auth-api",
  "version": "1.0.1",
  "description": "Provide auth services for Shingo APIs",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "shingo-auth-api-server": "./server/app.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "class-transformer": "^0.1.9",
    "grpc": "^1.12.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mysql": "^2.15.0",
    "protobufjs": "^6.8.8",
    "reflect-metadata": "^0.1.12",
    "scrypt": "^6.0.3",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.7",
    "typeorm-typedi-extensions": "^0.2.1",
    "winston": "^2.4.0"
  },
  "scripts": {
    "start": "node ./build/server/app.js",
    "debug": "nodemon --watch ./build --inspect=0.0.0.0:9229 --nolazy ./build/server/app.js",
    "docker-debug": "docker-compose -f docker-compose.debug.yml up",
    "proto": "mkdir -p build/proto && cp -R src/proto build/",
    "build": "rm -rf build/* && npm run proto && tsc -p ./tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -p . -c tslint.json",
    "prepack": "npm run build && cp -Rav build/* . | ./scripts/parseEmitted.sh build.emit",
    "postpack": "./scripts/postpack.sh"
  },
  "author": "Abraham White",
  "license": "MIT",
  "devDependencies": {
    "@shingo/tslint-rules": "^1.0.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.115",
    "@types/node": "^10.5.4",
    "@types/winston": "^2.3.9",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
