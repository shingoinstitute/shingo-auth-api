{
  "name": "@shingo/auth-api-shared",
  "version": "1.0.0-alpha.0",
  "description": "Shared library for shingo auth service",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shingoinstitute/shingo-auth-api.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:proto": "proto2ts ../../proto/auth_services.proto > src/auth_services.proto.ts",
    "build:main": "tsc -b .",
    "clean": "trash build test",
    "prepublishOnly": "npm run build"
  },
  "author": "Abraham White <abelincoln.white@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "grpc": "^1.15.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@shingo/proto2ts": "^0.1.1",
    "npm-run-all": "^4.1.3",
    "trash-cli": "^1.4.0",
    "typescript": "^3.0.3"
  }
}
