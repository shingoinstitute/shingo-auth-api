{
  "name": "@shingo/auth-api-server",
  "version": "1.0.0-alpha.1",
  "description": "Provide auth services for Shingo APIs",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shingoinstitute/shingo-auth-api.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "auth-api-server": "./app.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "@shingo/auth-api-shared": "^1.0.0-alpha.1",
    "class-transformer": "^0.1.9",
    "grpc": "^1.12.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mysql": "^2.15.0",
    "protobufjs": "^6.8.8",
    "reflect-metadata": "^0.1.12",
    "scrypt": "^6.0.3",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.7",
    "typeorm-typedi-extensions": "^0.2.1",
    "winston": "^2.4.0"
  },
  "scripts": {
    "start": "node ./build/app.js",
    "debug": "nodemon --watch ./build --inspect=0.0.0.0:9229 --nolazy ./build/app.js",
    "docker-debug": "docker-compose -f docker-compose.debug.yml up",
    "build": "run-s clean && run-p build:*",
    "build:proto": "mkdir -p build/proto && cp -R ../../proto build/",
    "build:main": "tsc -b .",
    "clean": "trash build test docs",
    "prepack": "npm run build && cp -Rav build/* . | ../../scripts/parseEmitted.sh build.emit",
    "postpack": "../../scripts/postpack.sh"
  },
  "author": "Abraham White",
  "license": "MIT",
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.115",
    "@types/node": "^10.5.4",
    "@types/scrypt": "^6.0.0",
    "@types/winston": "^2.3.9",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "trash-cli": "^1.4.0",
    "typescript": "^3.0.3"
  }
}
